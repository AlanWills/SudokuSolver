<UserControl x:Class="SudokuSolver.Editors.SudokuSolver.Views.SudokuElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SudokuSolver.Editors.SudokuSolver.Views"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="450">
    <xctk:IntegerUpDown Grid.Row="0" Grid.Column="0" Style="{StaticResource SudokuElementStyle}"
                        ShowButtonSpinner="False" Text="{Binding ValueString}"
                        ToolTip="{Binding PossibleValuesString}"/>
</UserControl>
